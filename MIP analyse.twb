      <column caption='groupe chaine' datatype='string' name='[Calculation_1528409185894404098]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='IF CONTAINS([ch], &quot;BFM TV&quot;) OR CONTAINS([ch], &quot;CNews&quot;) OR CONTAINS([ch], &quot;France 24&quot;) OR CONTAINS([ch], &quot;LCI&quot;) THEN &quot;Chaînes d&apos;infos&quot;&#13;&#10;ELSEIF CONTAINS([ch], &quot;TF1&quot;) OR CONTAINS([ch], &quot;France 2&quot;) OR CONTAINS([ch], &quot;France 3&quot;) OR CONTAINS([ch], &quot;M6&quot;) OR CONTAINS([ch], &quot;Canal +&quot;) OR CONTAINS([ch], &quot;W9&quot;) THEN &quot;Chaînes généralistes et de divertissement&quot;&#13;&#10;ELSEIF CONTAINS([ch], &quot;ARTE&quot;) OR CONTAINS([ch], &quot;France 5&quot;) OR CONTAINS([ch], &quot;La Cinquième&quot;) OR CONTAINS([ch], &quot;Planète&quot;) OR CONTAINS([ch], &quot;Toute l&apos;Histoire&quot;) THEN &quot;Chaînes culturelles et éducatives&quot;&#13;&#10;ELSEIF CONTAINS([ch], &quot;Gulli&quot;) OR CONTAINS([ch], &quot;Canal J&quot;) OR CONTAINS([ch], &quot;Télétoon&quot;) OR CONTAINS([ch], &quot;Disney Chanel&quot;) OR CONTAINS([ch], &quot;Jetix&quot;) THEN &quot;Chaînes pour enfants et dessins animés&quot;&#13;&#10;ELSEIF CONTAINS([ch], &quot;C8&quot;) OR CONTAINS([ch], &quot;TMC&quot;) OR CONTAINS([ch], &quot;TF1 Séries Films&quot;) OR CONTAINS([ch], &quot;TF6&quot;) OR CONTAINS([ch], &quot;NRJ 12&quot;) THEN &quot;Chaînes de divertissement spécialisées&quot;&#13;&#10;ELSEIF CONTAINS([ch], &quot;National Geographic&quot;) OR CONTAINS([ch], &quot;Discovery Channel&quot;) OR CONTAINS([ch], &quot;Science &amp; Vie TV&quot;) OR CONTAINS([ch], &quot;RMC Découverte&quot;) THEN &quot;Chaînes de documentaires et de découvertes&quot;&#13;&#10;ELSEIF CONTAINS([ch], &quot;L&apos;Equipe 21&quot;) OR CONTAINS([ch], &quot;Eurosport&quot;) OR CONTAINS([ch], &quot;beIN SPORTS&quot;) THEN &quot;Chaînes sportives&quot;&#13;&#10;ELSEIF CONTAINS([ch], &quot;MTV&quot;) OR CONTAINS([ch], &quot;MCM&quot;) OR CONTAINS([ch], &quot;Virgin 17&quot;) THEN &quot;Chaînes musicales&quot;&#13;&#10;ELSEIF CONTAINS([ch], &quot;TV5MONDE&quot;) OR CONTAINS([ch], &quot;Al Jazeera&quot;) OR CONTAINS([ch], &quot;BBC World News&quot;) OR CONTAINS([ch], &quot;CNN International&quot;) THEN &quot;Chaînes internationales et culturelles&quot;&#13;&#10;ELSEIF CONTAINS([ch], &quot;France 3&quot;) THEN &quot;Chaînes régionales&quot;&#13;&#10;ELSE &quot;Autres chaînes&quot;&#13;&#10;END' />
      </column>
      <column caption='choisir un résumé existant' datatype='string' name='[Calculation_193091896166891523]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='IF ISNULL([res]) AND ISNULL([resprod]) THEN NULL&#13;&#10;ELSEIF ISNULL([res]) THEN [resprod]&#13;&#10;ELSEIF ISNULL([resprod]) THEN [res]&#13;&#10;ELSE [res]&#13;&#10;END' />
